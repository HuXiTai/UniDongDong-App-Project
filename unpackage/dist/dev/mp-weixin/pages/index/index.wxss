.homeContainer {
  padding: 20rpx;
  box-sizing: border-box;
}
.homeContainer .header {
  display: flex;
  align-items: center;
}
.homeContainer .header .logo {
  width: 64rpx;
  height: 76rpx;
}
.homeContainer .header .search {
  display: flex;
  flex: 1;
  align-items: center;
  background-color: #f6f6f6;
  margin: 0 20rpx;
  border-radius: 10rpx;
}
.homeContainer .header .search .iconfont {
  margin: 0 30rpx 0 20rpx;
}
.homeContainer .header .hamburger {
  width: 44rpx;
  height: 76rpx;
}
.homeContainer .scrollList {
  height: calc(100vh - 156rpx);
}
.homeContainer .scrollList .banner .bannerList {
  width: 100%;
  height: 216rpx;
}
.homeContainer .scrollList .banner .bannerList .bannerItem {
  width: 100%;
  height: 216rpx;
}
.homeContainer .scrollList .banner .bannerList .bannerItem .bannerImg {
  margin-top: 20rpx;
  width: 100%;
  height: 216rpx;
}
.homeContainer .scrollList .typeList {
  margin-top: 20rpx;
}
.homeContainer .scrollList .typeList .typeItem {
  display: flex;
  flex-wrap: wrap;
  width: 300rpx;
}
.homeContainer .scrollList .typeList .typeItem .typeImg {
  height: 156rpx;
  width: 20%;
}
.homeContainer .scrollList .brandRush .header {
  height: 80rpx;
  display: flex;
  align-items: center;
}
.homeContainer .scrollList .brandRush .header .text {
  line-height: 80rpx;
  margin: 0 20rpx;
}
.homeContainer .scrollList .brandRush .header .headerImg {
  margin-top: 5rpx;
  width: 144rpx;
  height: 32rpx;
  vertical-align: middle;
}
.homeContainer .scrollList .brandRush .body {
  display: flex;
  justify-content: space-evenly;
}
.homeContainer .scrollList .brandRush .body .bodyImg {
  width: 158rpx;
  height: 158rpx;
}

